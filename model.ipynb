      "source": [
        "import pandas as pd\n",
        "from surprise import Dataset, Reader, SVD\n",
        "from surprise.model_selection import train_test_split\n",
        "from surprise import accuracy"
      ],
      "metadata": {
        "id": "GDCBTaBKJ0CD"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "df = pd.read_csv('/content/taif_user_place_ratings.csv')  # <-- غيري المسار حسب مكان الملف\n",
        "\n",
        "# 4. إعداد البيانات لـ Surprise\n",
        "reader = Reader(rating_scale=(1, 5))\n",
        "data = Dataset.load_from_df(df[['user_id', 'place_name', 'rating']], reader)\n",
        "\n",
        "# 5. تقسيم البيانات\n",
        "trainset, testset = train_test_split(data, test_size=0.25, random_state=42)\n",
        "\n",
        "# 6. تدريب النموذج باستخدام SVD\n",
        "model = SVD()\n",
        "model.fit(trainset)\n",
        "\n",
        "# 7. تقييم النموذج\n",
        "predictions = model.test(testset)\n",
        "accuracy.rmse(predictions)\n",
        "\n",
        "# 8. عمل توصيات\n",
        "user_ids = df['user_id'].unique()\n",
        "place_names = df['place_name'].unique()\n",
        "recommendations = []\n",
        "\n",
        "for user in user_ids:\n",
        "    for place in place_names:\n",
        "        if not ((df['user_id'] == user) & (df['place_name'] == place)).any():\n",
        "            est = model.predict(user, place).est\n",
        "            recommendations.append((user, place, round(est, 2)))\n",
        "\n",
        "# 9. تجهيز جدول التوصيات\n",
        "rec_df = pd.DataFrame(recommendations, columns=['user_id', 'place_name', 'estimated_rating'])\n",
        "top5 = rec_df.sort_values(by=['user_id', 'estimated_rating'], ascending=[True, False])\n",
        "top5 = top5.groupby('user_id').head(5)\n",
        "\n",
        "# 10. حفظ التوصيات في ملف CSV\n",
        "top5.to_csv('/content/taif_top5_recommendations.csv', index=False)\n",
        "\n",
        "# عرض أول 10 توصيات\n",
        "top5.head(10)"
      ],
