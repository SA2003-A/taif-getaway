        "\n",
        "!pip install scikit-surprise --quiet\n",
        "\n",
        "# استيراد المكتبات\n",
        "import pandas as pd\n",
        "from surprise import Dataset, Reader, SVD\n",
        "from surprise.model_selection import train_test_split\n",
        "from surprise import accuracy\n",
        "\n",
        "# إنشاء بيانات وهمية لتجربة النموذج\n",
        "data_dict = {\n",
        "    'user_id': [1, 1, 1, 2, 2, 3, 3, 4, 4, 4],\n",
        "    'place_id': [101, 102, 103, 101, 104, 102, 105, 103, 104, 105],\n",
        "    'rating': [5, 4, 3, 4, 2, 5, 3, 2, 4, 5]\n",
        "}\n",
        "df = pd.DataFrame(data_dict)\n",
        "\n",
        "# عرض البيانات\n",
        "print(\"عرض البيانات:\")\n",
        "print(df.head())\n",
        "\n",
        "# قراءة البيانات باستخدام Surprise\n",
        "reader = Reader(rating_scale=(1, 5))\n",
        "data = Dataset.load_from_df(df[['user_id', 'place_id', 'rating']], reader)\n",
        "\n",
        "# تقسيم البيانات إلى تدريب واختبار\n",
        "trainset, testset = train_test_split(data, test_size=0.25)\n",
        "\n",
        "# تدريب النموذج باستخدام خوارزمية SVD\n",
        "model = SVD()\n",
        "model.fit(trainset)\n",
        "\n",
        "# التنبؤ بالتقييمات على بيانات الاختبار\n",
        "predictions = model.test(testset)\n",
        "\n",
        "# حساب الدقة\n",
        "rmse = accuracy.rmse(predictions)\n",
        "print(f\"RMSE: {rmse}\")\n",
        "\n",
        "# توصية لمستخدم معيّن (مثلاً user_id = 1)\n",
        "user_id = 1\n",
        "places = df['place_id'].unique()\n",
        "\n",
        "print(\"\\nتوصيات للمستخدم 1:\")\n",
        "for place_id in places:\n",
        "    pred = model.predict(uid=user_id, iid=place_id)\n",
        "    print(f\"Place {place_id}: {pred.est:.2f}\")"
      ],
